<% if !@incompanies.nil? %>
  <table cellpadding="20" sortable>
    <tr>
      <th style="width: 20%">first_name</th>
      <th style="width: 20%">last_name</th>
      <th style="width: 20%">company_name</th>
      <th style="width: 20%">email</th>
      <th style="width: 20%">date</th>
      <th style="width: 20%">View</th>
      <th style="width: 20%">Edit</th>
      <th style="width: 20%">Delete</th>
    </tr>
    <% @incompanies.each do |incompany|%>
    <tr>
      <td style="width: 20%"><%= incompany.first_name%></td>
      <td style="width: 20%"><%= incompany.last_name%></td>
      <td style="width: 20%"><%= incompany.company_name%></td>
      <td style="width: 20%"><%= incompany.email%></td>
      <td style="width: 20%"><%= incompany.date%></td>
      <td style="width: 20%"><%= link_to "View", admin_incompany_path(incompany) %></td>
      <td style="width: 20%"><%= link_to "Edit", edit_admin_incompany_path(incompany) %></td>
      <td style="width: 20%"><%= link_to "Delete", admin_incompany_path(incompany), method: :delete, data: {confirm: "Are you sure?"}%></td>
    </tr>
    <% end %>
  </table>
<% end %>
