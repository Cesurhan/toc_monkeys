<%= form_for @sponsor do |f| %>
<div class="section">
  <div class="col-md-4 col-md-offset-4">
    <div class="form-area">
      <form role="form">
        <br style="clear:both">
        <h2 style="margin-bottom: 25px; text-align: center;">Book TOC</h2>
        <h3 style="margin-bottom: 25px; text-align: center;">Talk to us</h3>
        <div class="form-group">
          <%= f.text_field :first_name, class: "form-control", placeholder: "First Name" %>
        </div>
        <div class="form-group">
          <%= f.text_field :last_name, class: "form-control", placeholder: "Last Name"%>
        </div>
        <div class="form-group">
          <%= f.text_field :company_name, class: "form-control", placeholder: "Company Name"%>
        </div>
        <div class="form-group">
          <%= f.text_field :email, class: "form-control", placeholder: "Email"%>
        </div>
        <div class="form-group">
          <span class="input-group registration-date-time">
            <%= f.date_field :date, class: "form-control", placeholder: "Date event" %>
          </span>
        </div>
        <div class="form-group">
          <%=f.number_field :spaces_available, class: "form-control", placeholder: "Number of participants" %>
        </div>
      </div>
      <div class="form-group">
        <%=f.text_area :comments, class:"form-control", :rows => 9, placeholder: "Comments"%>
        <span class="help-block">
          <p id="characterLeft" class="help-block ">You have reached the limit</p>
        </span>
      </div>
      <div class="actions text-center">
        <button type="button" id="submit" name="submit" class="btn btn-primary">Submit Form</button>
      </div>
    </form>
  </div>
</div>
</div>
<%end%>
