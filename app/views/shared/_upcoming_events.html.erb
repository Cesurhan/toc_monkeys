<h1>
  Upcoming Events!
</h1>

<section class="row">
  <% @events.each do |event| %>
  <div class="col-md-4">
    <div class="card-block">
      <img src="https://res.cloudinary.com/dnbyfobad/image/upload/v1490716457/event1_iweuk3.jpg" height=50/>
      <h3 class="card-title"><%= event.title %></h3>
      <p class="card-text"><%= event.location %></p>

      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
  </section>
  <% end %>

</div>
